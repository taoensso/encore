{:lein true
 :nrepl    {:port 7887 :middleware [cider.nrepl/cider-middleware]}
 :dev-http {8090 {:root "public"}}
 :builds
 {:main
  {:target :browser
   :output-dir "public/js"
   :modules  {:main {:entries [taoensso.encore]}}
   :preloads [#_devtools.preload]}

  :tests
  {:target :node-test
   :output-to "target/tests.js"
   :ns-regexp "-tests$"
   :autorun true}}}
